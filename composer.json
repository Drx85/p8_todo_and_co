{
    "name": "saro0h/to-do-list",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
		  "AppBundle\\": "src/AppBundle",
		  "App\\": "src/"
		}
    },
    "autoload-dev": {
        "psr-4": {
		  "Tests\\AppBundle\\": "tests/AppBundle",
		  "App\\Tests\\": "tests/"
		}
    },
    "require": {
        "php": ">=7.2.5",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/migrations": "^3.2",
        "doctrine/orm": "2.9.5",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "*",
        "symfony/console": "^5.3.7",
        "symfony/flex": "^1.15",
        "symfony/framework-bundle": "^5.3.7",
        "symfony/maker-bundle": "^1.33",
        "symfony/monolog-bundle": "3.7.0",
        "symfony/polyfill-apcu": "^1.23",
        "symfony/runtime": "5.3.4",
        "symfony/security-bundle": "^5.3.7",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/translation": "^5.3.7",
        "symfony/twig-bundle": "^5.3.4",
        "symfony/yaml": "5.3.6",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "symfony/dotenv": "5.3.7",
        "symfony/phpunit-bridge": "*",
        "symfony/stopwatch": "^5.3.4",
        "symfony/web-profiler-bundle": "^5.3.5"
    },
 	 "config": {
		"optimize-autoloader": true,
		"preferred-install": {
	  		"*": "dist"
	},
	"sort-packages": true
  },
  	"scripts": {
		"auto-scripts": [
	],
		"post-install-cmd": [
	  		"@auto-scripts"
	],
		"post-update-cmd": [
	  		"@auto-scripts"
	]
  },
  	"extra": {
		"symfony": {
	  		"allow-contrib": false
	}
  }
}
